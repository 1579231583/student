AAA,SAAA,CAAA,WAHA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA;AACA,OANA;AAOA,KAnBA;AAoBA,IAAA,IApBA,gBAoBA,EApBA,EAoBA;AAAA;;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,OAHA;AAIA,KAzBA;AA0BA,IAAA,IA1BA,kBA0BA;AAAA;;AACA,MAAA,GAAA,CAAA,MAAA,CAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAJA;AAKA;AAhCA,GAjBA;AAmDA,EAAA,OAnDA,qBAmDA;AACA,SAAA,KAAA;AACA,GArDA;AAsDA,EAAA,OAtDA,qBAsDA;AAAA;;AACA,IAAA,SAAA,CAAA,oBAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,UAAA,GAAA,GAAA;AACA,KAFA;AAGA;AA1DA,CAAA","sourcesContent":["<template>\n    <div class=\"grade-wrap\">\n        <div class=\"crumbs\">\n            <el-breadcrumb separator=\"/\">\n                <el-breadcrumb-item>\n                    <b style=\"font-size: 22px\"> 成绩录入</b>\n                </el-breadcrumb-item>\n            </el-breadcrumb>\n        </div>\n\n        <div class=\"container\">\n            <div class=\"query-form\">\n                <el-row :gutter=\"20\">\n                    <el-col :offset=\"15\" :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"课程名\"\n                            v-model=\"queryForm.courseName\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"学生名\"\n                            v-model=\"queryForm.studentName\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-button\n                            @click=\"query\"\n                            type=\"primary\"\n                            style=\"font-size: 18px\"\n                        >\n                            搜索\n                        </el-button>\n                    </el-col>\n                </el-row>\n            </div>\n\n            <div class=\"table\">\n                <el-table :data=\"tableData\" stripe>\n                    <el-table-column label=\"选课Id\" prop=\"studentCourseId\" />\n                    <el-table-column label=\"课程名\" prop=\"courseName\" />\n                    <el-table-column label=\"学生姓名\" prop=\"studentName\" />\n                    <el-table-column label=\"日常分\" prop=\"dailyScore\" />\n                    <el-table-column label=\"笔试分\" prop=\"examScore\" />\n                    <el-table-column label=\"成绩\" prop=\"score\" />\n                    <el-table-column\n                        align=\"center\"\n                        label=\"操作\"\n                        width=\"200px\"\n                        fixed=\"right\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-button\n                                @click=\"edit(scope.row.studentCourseId)\"\n                                type=\"success\"\n                                style=\"font-size: 18px\"\n                                :disabled=\"!allowGrade\"\n                            >\n                                评分\n                            </el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n            </div>\n\n            <el-row justify=\"center\" type=\"flex\" style=\"margin: 5px 0 5px 0\">\n                <el-pagination\n                    :current-page.sync=\"pageIndex\"\n                    :page-size=\"pageSize\"\n                    :total=\"pageSize * pageCount\"\n                    @current-change=\"getPage\"\n                    background\n                    layout=\"prev, pager, next\"\n                >\n                </el-pagination>\n            </el-row>\n\n            <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n                <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n                    <el-form-item label=\"选课Id\">\n                        <el-input\n                            disabled\n                            type=\"number\"\n                            v-model=\"entityForm.studentCourseId\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"日常成绩\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.dailyScore\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"笔试成绩\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.examScore\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"总成绩\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.score\"\n                        ></el-input>\n                    </el-form-item>\n                </el-form>\n                <span class=\"dialog-footer\" slot=\"footer\">\n                    <el-button\n                        @click=\"save\"\n                        type=\"primary\"\n                        style=\"font-size: 18px\"\n                        >\n                        确 定</el-button\n                    >\n                    <el-button @click=\"editing = false\" style=\"font-size: 18px\"\n                        >\n                        取 消</el-button\n                    >\n                </span>\n            </el-dialog>\n        </div>\n    </div>\n</template>\n\n<script>\nimport * as api from \"../../api/teacher/grade\";\nimport * as OptionApi from \"../../api/option\";\n\nexport default {\n    name: \"TeacherGrade\",\n    data() {\n        return {\n            queryForm: {\n                courseName: \"\",\n                studentName: \"\",\n            },\n            entityForm: {},\n            tableData: [],\n            pageSize: api.pageSize,\n            pageCount: 1,\n            pageIndex: 1,\n            editing: false,\n            allowGrade: true,\n        };\n    },\n    methods: {\n        query() {\n            api.getPageCount(\n                this.queryForm.courseName,\n                this.queryForm.studentName\n            ).then((res) => {\n                this.pageCount = res;\n                this.pageIndex = 1;\n                this.getPage(1);\n            });\n        },\n        getPage(pageIndex) {\n            api.getPage(\n                pageIndex,\n                this.queryForm.courseName,\n                this.queryForm.studentName\n            ).then((res) => {\n                this.tableData = res;\n            });\n        },\n        edit(id) {\n            api.get(id).then((res) => {\n                this.entityForm = res;\n                this.editing = true;\n            });\n        },\n        save() {\n            api.update(this.entityForm).then(() => {\n                this.$message.success(\"成功\");\n                this.getPage(this.pageIndex);\n                this.editing = false;\n            });\n        },\n    },\n    created() {\n        this.query();\n    },\n    mounted() {\n        OptionApi.getAllowTeacherGrade().then((res) => {\n            this.allowGrade = res;\n        });\n    },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views/teacher"}]}