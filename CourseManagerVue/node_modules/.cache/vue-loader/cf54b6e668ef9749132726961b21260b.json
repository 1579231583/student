\n    <div class=\"info-wrap\">\n        <div class=\"crumbs\">\n            <el-breadcrumb separator=\"/\">\n                <el-breadcrumb-item>\n                    <b style=\"font-size: 22px\"> 信息维护</b>\n                </el-breadcrumb-item>\n            </el-breadcrumb>\n        </div>\n\n        <div class=\"container\">\n            <el-form :model=\"entityForm\" class=\"info-form\" label-width=\"80px\">\n                <el-form-item label=\"学号\">\n                    <el-input disabled v-model=\"entityForm.number\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"姓名\">\n                    <el-input disabled v-model=\"entityForm.name\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"教学系\">\n                    <el-input\n                        disabled\n                        v-model=\"entityForm.departmentName\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"专业\">\n                    <el-input\n                        disabled\n                        v-model=\"entityForm.majorName\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"班级\">\n                    <el-input\n                        disabled\n                        v-model=\"entityForm.className\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"Email\">\n                    <el-input v-model=\"entityForm.email\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"生日\">\n                    <el-date-picker\n                        format=\"yyyy-MM-dd\"\n                        placeholder=\"选择生日\"\n                        type=\"date\"\n                        v-model=\"entityForm.birthday\"\n                    >\n                    </el-date-picker>\n                </el-form-item>\n                <el-form-item label=\"性别\">\n                    <el-radio-group v-model=\"entityForm.sex\">\n                        <el-radio :label=\"1\">男</el-radio>\n                        <el-radio :label=\"0\">女</el-radio>\n                    </el-radio-group>\n                </el-form-item>\n                <el-form-item label=\"密码\">\n                    <el-input\n                        type=\"password\"\n                        v-model=\"entityForm.password\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"操作\">\n                    <el-button\n                        @click=\"update\"\n                        type=\"primary\"\n                        style=\"font-size: 18px\"\n                        ><i\n                            class=\"iconfont icon-r-yes\"\n                            style=\"font-size: 22px\"\n                        ></i>\n                        确认更新</el-button\n                    >\n                </el-form-item>\n            </el-form>\n        </div>\n    </div>\n</template>\n\n<script>\nimport * as api from \"../../api/student/info\";\n\nexport default {\n    name: \"StudentInfo\",\n    data() {\n        return {\n            entityForm: {\n                number: \"\",\n                name: \"\",\n                departmentName: \"\",\n                majorName: \"\",\n                className: \"\",\n                email: \"\",\n                birthday: \"\",\n                sex: \"\",\n                password: null,\n            },\n        };\n    },\n    methods: {\n        get() {\n            api.get().then((res) => {\n                this.entityForm = res;\n            });\n        },\n        update() {\n            api.update(this.entityForm).then(() => {\n                this.$message.success(\"更新信息成功!\");\n            });\n        },\n    },\n    created() {\n        this.get();\n    },\n};\n</script>\n\n<style scoped>\n.info-form {\n    min-width: 400px;\n    width: 35%;\n    margin: auto;\n}\n</style>\n"]}]}